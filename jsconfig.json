{
    "compilerOptions": {
        "checkJs": true,
        "target": "ES2020",
        "module": "ES2020",
        "moduleResolution": "node",
        "strict": false,
        "baseUrl": "./src",
        "paths": {
            "*": ["*"]
        },
        "typeRoots": [
            "./src/types"
        ]
    },
    "include": [
        "src/**/*"
    ],
    "exclude": [
        "node_modules",
        "dist"
    ]
}
